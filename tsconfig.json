{
	"extends": "./.svelte-kit/tsconfig.json",
	"include": ["svelte.config.js", "vite.config.ts", "src/**/*"],
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	}
}
